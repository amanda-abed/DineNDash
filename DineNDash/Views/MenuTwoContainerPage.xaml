<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="DineNDash.Views.MenuTwoContainerPage"
    xmlns:views="clr-namespace:DineNDash.Views">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Icon="shoppingcart.png" Command="{Binding GoToCart}"/>
    </TabbedPage.ToolbarItems>
    <views:TabSandwichesPage 
        Title="Sandwiches"
        Icon="sub.png"
        />
    <views:TabSideAndDrinksPage
        Title="Sides &amp; Drinks"
        Icon="drink.png"
        />
</TabbedPage>
